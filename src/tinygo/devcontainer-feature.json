{          
    "id": "tinygo",
    "version": "1.0.3",
    "name": "Tiny Go",
    "options": {
        "version": {
            "type": "string",
            "default": "latest",
            "description": "Specify a version of Tiny Go"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "tinygo.vscode-tinygo",
                "golang.go"
            ],
            "features": {
                "ghcr.io/devcontainers/features/go:1": {}
            }
        }
    }
}